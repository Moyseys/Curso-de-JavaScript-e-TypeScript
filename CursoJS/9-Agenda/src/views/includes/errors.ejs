<%if(error.length > 0){%>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-danger">
                <%error.forEach(erro => { %>
                    <%= erro %> <br>
                <% }) %>
            </div>
        </div>
    </div>
<% } %>

<%if(sucess.length > 0){%>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-success">
                <%sucess.forEach(erro => { %>
                    <%= erro %> <br>
                <% }) %>
            </div>
        </div>
    </div>
<% } %>